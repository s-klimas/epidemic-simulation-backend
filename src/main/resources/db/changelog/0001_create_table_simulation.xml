<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">
    <changeSet id="1" author="sebastianklimas">
        <createTable tableName="simulation">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="n" type="VARCHAR(100)"/>
            <column name="p" type="INT"/>
            <column name="i" type="INT"/>
            <column name="r" type="DECIMAL"/>
            <column name="m" type="DECIMAL"/>
            <column name="ti" type="INT"/>
            <column name="tm" type="INT"/>
            <column name="ts" type="INT"/>
        </createTable>
    </changeSet>
</databaseChangeLog>